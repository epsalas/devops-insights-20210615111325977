<form asp-action="CrearReclamo">
  <div asp-validation-summary="ModelOnly" class="text-danger"></div>
  <div class="form-group">
    <label name="usuario">Ingrese sus nombres completos</label><br/>
    <input asp-for="usuario" class="form-control"/><br/>
    
  </div>
  <div>
      <label name="comentario">Mensaje</label><br/>
  </div>
    <input asp-for="comentario"  class="form-control"/><br/>

  <div class="form-group">
    <label name="fecha">Ingrese la Fecha del pedido</label><br/>
    <input asp-for="fecha"  class="form-control" type="date"/>            
  </div>
  </br>

  <input type="submit" value="Enviar" class="btn btn-primary" />
</form>
